{
    "name": "bleuren/jetstream-livewire-i18n",
    "description": "A Laravel package to add multi-language support to Jetstream with Livewire",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bleuren",
            "email": "bleu@hiverz.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/jetstream": "^5.3",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Bleuren\\JetstreamLivewireI18n\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bleuren\\JetstreamLivewireI18n\\Providers\\JetstreamLivewireI18nServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}